{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Embed Demo Text",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build-console",
            "program": "${workspaceFolder}/MiddleSchool.ConsoleApp/bin/Debug/net8.0/MiddleSchool.ConsoleApp.dll",
            "args": [
                "embed",
                "Text\\Demo.txt"
            ],
            "cwd": "${workspaceFolder}/MiddleSchool.ConsoleApp/bin/Debug/net8.0",
            "console": "internalConsole",
            "env": {
                "DOTNET_ENVIRONMENT": "Development"
            },
            "stopAtEntry": false
        },
